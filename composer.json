{
    "name": "silverstripe/recipe-core",
    "type": "silverstripe-recipe",
    "description": "SilverStripe framework-only core recipe",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/assets": "1.0.0@stable",
        "silverstripe/config": "1.0.0@stable",
        "silverstripe/framework": "4.0.0@stable",
        "silverstripe/graphql": "^1.0",
        "silverstripe/graphql-devtools": "dev-master",
        "silverstripe/sqlite3": "dev-master",
        "silverstripe/versioned": "^1.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "fzaninotto/faker": "^1.7"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MyOrg\\": "app/src/"
        }
    }
}
